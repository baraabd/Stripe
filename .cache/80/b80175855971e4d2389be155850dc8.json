ge.receipt_url)\n    }\n\n    if (receiptUrl) {\n        return (\n          <div className=\"success\">\n            <h2>Payment Successful!</h2>\n            <a href={receiptUrl}>View Receipt</a>\n            <Link to=\"/\">Home</Link>\n          </div>\n        )\n      }\n      return (\n        <div className=\"checkout-form\">\n          <p>Amount: ${selectedProduct.price}</p>\n          <form onSubmit={handleSubmit}>\n            <label>\n              Card details\n              <CardNumberElement />\n            </label>\n            <label>\n              Expiration date\n              <CardExpiryElement />\n            </label>\n            <label>\n              CVC\n              <CardCVCElement />\n            </label>\n            <button type=\"submit\" className=\"order-button\">\n              Pay\n            </button>\n          </form>\n        </div>\n      )\n    }\n    \n    export default injectStripe(CheckoutForm)"},"lineCount":null}},"error":null,"hash":"20bc6971252e2e82271343e09d49642d","cacheData":{"env":{}}}